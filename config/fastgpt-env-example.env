# FastGPT 模拟配置环境变量示例
# 复制此文件为 .fastgpt-env 并填入实际值

# FastGPT API 配置
# ===================

# FastGPT 售后服务助手配置
FASTGPT_API_KEY_1=sk-fastgpt-customer-service-key-here
FASTGPT_APP_ID_1=app-customer-service-id-here

# FastGPT 配单智能应用配置
FASTGPT_API_KEY_2=sk-fastgpt-order-system-key-here
FASTGPT_APP_ID_2=app-order-system-id-here

# FastGPT 需求分析助手配置
FASTGPT_API_KEY_3=sk-fastgpt-requirement-analysis-key-here
FASTGPT_APP_ID_3=app-requirement-analysis-id-here

# FastGPT 服务端点配置
FASTGPT_ENDPOINT=https://api.fastgpt.run/v1
FASTGPT_BASE_URL=https://api.fastgpt.run

# FastGPT 模型配置
FASTGPT_DEFAULT_MODEL=FastAI-4k
FASTGPT_MAX_TOKENS=4096
FASTGPT_TEMPERATURE=0.7

# FastGPT 速率限制配置
FASTGPT_RATE_LIMIT_REQUESTS_PER_MINUTE=60
FASTGPT_RATE_LIMIT_TOKENS_PER_MINUTE=40000

# FastGPT 流式响应配置
FASTGPT_STREAM_ENABLED=true
FASTGPT_STREAM_CHUNK_SIZE=100
FASTGPT_STREAM_TIMEOUT=30000

# FastGPT 缓存配置
FASTGPT_CACHE_ENABLED=true
FASTGPT_CACHE_TTL=300
FASTGPT_CACHE_MAX_SIZE=1000

# FastGPT 监控配置
FASTGPT_MONITORING_ENABLED=true
FASTGPT_HEALTH_CHECK_INTERVAL=60
FASTGPT_PERFORMANCE_TRACKING=true

# FastGPT 安全配置
FASTGPT_WEBHOOK_SECRET=your-webhook-secret-here
FASTGPT_ENCRYPTION_KEY=your-encryption-key-here

# 开发调试配置
# ===================
FASTGPT_DEBUG=false
FASTGPT_LOG_LEVEL=info
FASTGPT_SIMULATION_MODE=false

# 模拟配置（用于开发测试）
# ===================
SIMULATE_RESPONSES=true
SIMULATE_DELAY_MIN=500
SIMULATE_DELAY_MAX=2000
SIMULATE_ERROR_RATE=0.05
SIMULATE_CACHE_HIT_RATE=0.3

# 配置说明：
# 1. API密钥和AppID需要从FastGPT管理后台获取
# 2. 根据实际部署情况调整端点URL
# 3. 速率限制需要根据FastGPT服务套餐调整
# 4. 生产环境请关闭SIMULATE_*相关配置