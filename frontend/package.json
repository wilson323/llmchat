{
  "name": "@llmchat/frontend",
  "version": "1.0.0",
  "description": "LLMChat前端应用 - 智能体切换聊天界面",
  "type": "module",
  "scripts": {
    "dev": "node ../scripts/find-backend-port.js && vite",
    "dev:auto": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "lint:check": "node scripts/eslint-quality-check.cjs",
    "lint:quality": "node scripts/eslint-quality-check.cjs --report",
    "lint:auto-fix": "node scripts/eslint-auto-fix.cjs",
    "lint:sync": "node scripts/typescript-eslint-sync.cjs",
    "lint:sync:fix": "node scripts/typescript-eslint-sync.cjs --fix",
    "lint:sync:report": "node scripts/typescript-eslint-sync.cjs --report",
    "quality:check": "node scripts/typescript-eslint-sync.cjs --mode full",
    "lint:auto-fix:safe": "node scripts/eslint-auto-fix.cjs --strategy safe",
    "lint:auto-fix:aggressive": "node scripts/eslint-auto-fix.cjs --strategy aggressive",
    "lint:dry-run": "node scripts/eslint-auto-fix.cjs --dry-run",
    "lint:all": "npm run lint && npm run type-check && npm run lint:check",
    "lint:fix-all": "npm run lint:auto-fix && npm run format:code",
    "type-check": "tsc --noEmit -p tsconfig.json",
    "type-check:dev": "tsc --noEmit -p tsconfig.dev.json",
    "type-check:perf": "tsc --noEmit --project tsconfig.performance.json",
    "type-check:fast": "tsc --noEmit -p tsconfig.fast.json",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "type-safety-check": "node scripts/type-safety-check.js",
    "type-safety-check:strict": "node scripts/type-safety-check.js --strict",
    "type-safety-check:create-baseline": "node scripts/type-safety-check.js --create-baseline",
    "type-coverage": "pnpm exec type-coverage --detail",
    "type-coverage:monitor": "node scripts/type-coverage-monitor.js",
    "type-coverage:monitor:strict": "node scripts/type-coverage-monitor.js --strict --threshold 95",
    "type-regression-test": "node scripts/type-regression-tester.js",
    "type-regression-test:strict": "node scripts/type-regression-tester.js --strict",
    "multi-env:validate": "node scripts/multi-env-validator.js",
    "multi-env:validate:production": "node scripts/multi-env-validator.js --env production",
    "type-safety:report": "node scripts/type-safety-reporter.js",
    "type-safety:report:html": "node scripts/type-safety-reporter.js --format html",
    "type-safety:report:markdown": "node scripts/type-safety-reporter.js --format markdown",
    "type-safety:full-check": "node scripts/type-safety-check.js --strict && node scripts/type-coverage-monitor.js --threshold 90 && node scripts/type-regression-tester.js",
    "perf:monitor": "node scripts/performance-compiler-monitor.js",
    "perf:monitor:dev": "node scripts/performance-compiler-monitor.js --config=tsconfig.dev.json",
    "perf:monitor:perf": "node scripts/performance-compiler-monitor.js --config=tsconfig.performance.json --save",
    "perf:benchmark": "node scripts/performance-benchmark.js",
    "perf:validate": "node scripts/validate-performance-config.js",
    "perf:validate:fix": "node scripts/validate-performance-config.js --fix",
    "eslint:autofix": "node scripts/eslint-autofix.js",
    "eslint:autofix:dry-run": "node scripts/eslint-autofix.js --dry-run",
    "fix:magic-numbers": "node scripts/magic-number-fixer.js",
    "fix:magic-numbers:dry-run": "node scripts/magic-number-fixer.js --dry-run",
    "fix:unsafe-types": "node scripts/unsafe-type-fixer.js",
    "fix:unsafe-types:dry-run": "node scripts/unsafe-type-fixer.js --dry-run",
    "format:code": "node scripts/code-formatter.js",
    "format:code:dry-run": "node scripts/code-formatter.js --dry-run",
    "fix:validate": "node scripts/fix-validator.js",
    "fix:all": "node scripts/eslint-auto-fix-all.js",
    "fix:all:dry-run": "node scripts/eslint-auto-fix-all.js --dry-run",
    "fix:all:safe": "node scripts/eslint-auto-fix-all.js --fix --strategy safe",
    "fix:all:aggressive": "node scripts/eslint-auto-fix-all.js --fix --strategy aggressive",
    "fix:restore": "node scripts/eslint-auto-fix-all.js --restore",
    "perf:quick": "node scripts/quick-perf-test.js",
    "build:perf": "vite build --config vite.performance.config.ts",
    "quality:check-all": "npm run type-check && npm run lint && node scripts/duplicate-export-checker.cjs && node scripts/file-size-limiter.cjs",
    "pre-commit": "npm run quality:check-all"
  },
  "dependencies": {
    "@llmchat/shared-types": "workspace:*",
    "@radix-ui/react-slot": "^1.2.3",
    "@react-three/drei": "^9.92.0",
    "@react-three/fiber": "^8.15.0",
    "@sentry/react": "^10.17.0",
    "@sentry/tracing": "^7.120.4",
    "@tanstack/react-virtual": "^3.13.12",
    "@types/dompurify": "^3.2.0",
    "axios": "^1.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.0",
    "dxf-viewer": "^1.0.43",
    "echarts": "^5.6.0",
    "echarts-countries-js": "^1.0.5",
    "echarts-for-react": "^3.0.2",
    "folder-hash": "^4.0.4",
    "framer-motion": "^12.23.21",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.295.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hot-toast": "^2.6.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.9.2",
    "recharts": "^3.2.1",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^2.2.0",
    "three": "^0.160.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/three": "^0.160.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.2.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "i18next": "^25.5.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.32",
    "prettier": "^3.3.0",
    "react-i18next": "^16.0.0",
    "tailwindcss": "^4.1.13",
    "terser": "^5.44.0",
    "type-coverage": "^2.28.4",
    "typescript": "^5.9.3",
    "vite": "^5.0.10",
    "vitest": "^3.2.4",
    "web-vitals": "^5.1.0"
  }
}